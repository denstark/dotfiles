# vi:syntax=sh
alias ll="ls -alh --color"
alias ls="ls --color"
alias cul="docker compose up -d; docker compose logs -f"
alias bs="cd /home/tstella/repos/bootstrap/$(hostname -s ) 2> /dev/null || cd /home/tstella/repos/bootstrap/"
alias dotf="cd /home/$USER/repos/dotfiles/"
alias network="vim /home/tstella/repos/bootstrap/network"
alias pacman="sudo pacman"
alias dnf="sudo dnf"
alias apk="sudo apk"
